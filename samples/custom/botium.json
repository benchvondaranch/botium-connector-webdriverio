{
  "botium": {
    "Capabilities": {
      "PROJECTNAME": "WebdriverIO Plugin Sample",
      "CONTAINERMODE": "webdriverio",
      "CLEANUPTEMPDIR": false,
      "WEBDRIVERIO_URL": "https://www.drei.at",
      "_WEBDRIVERIO_OPENBOT": "./snippets/openbot",
      "__WEBDRIVERIO_OPENBOT": "./snippets/openbot_promises",
      "___WEBDRIVERIO_OPENBOT": "result = browser.$('#onetrust-accept-btn-handler').then(ccBtn => ccBtn.waitForClickable({ timeout: 20000 }).then(() => ccBtn.click())).then(() => browser.$('.troy__start-icon')).then(startChat => startChat.waitForClickable({ timeout: 20000 }).then(() => startChat.click()))",
      "WEBDRIVERIO_INPUT_ELEMENT": "#textInput",
      "WEBDRIVERIO_INPUT_NAVIGATION_BUTTONS": [ "#onetrust-accept-btn-handler", ".troy__start-icon" ],
      "WEBDRIVERIO_INPUT_ELEMENT_SENDBUTTON": ".troy__send",
      "WEBDRIVERIO_OUTPUT_ELEMENT": ".from-watson",
      "WEBDRIVERIO_OUTPUT_ELEMENT_HASH": "HASH",
      "WEBDRIVERIO_OUTPUT_ELEMENT_HASH_SELECTOR": "./div[contains(@class,'message-inner')]/div[contains(@class,'text')]",
      "WEBDRIVERIO_IGNOREWELCOMEMESSAGES": 1,
      "WEBDRIVERIO_START_CHROMEDRIVER": true
    }
  }
}
